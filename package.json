{
  "name": "react-commentview",
  "version": "1.0.51",
  "description": "A reusable react component to show comments or discussions on a particular topic",
  "author": "superflows-dev",
  "license": "MIT",
  "repository": "superflows-dev/react-commentview",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom --verbose=true --coverage",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "peerDependencies": {
    "react": "^16.0.0||^17.0.0||^18.0.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.1.2",
    "babel-eslint": "^10.0.3",
    "cross-env": "^7.0.3",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.31.0",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^2.2.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.7.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-scripts": "^3.4.4"
  },
  "dependencies": {
    "bootstrap": "^5.2.0",
    "emoji-picker-react": "^3.6.1",
    "react-bootstrap": "^2.5.0",
    "react-bootstrap-icons": "^1.8.4",
    "react-ui-components-superflows": "^1.0.30",
    "react-ui-themes-superflows": "^1.0.17",
    "react-upload-to-s3": "^1.0.12"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "react",
    "superflows",
    "comments",
    "discussions",
    "social",
    "chat",
    "like",
    "vote",
    "upvote",
    "video",
    "hls",
    "mediaconvert"
  ]
}
